{
    "manifest_version": 2,
    "name": "Test_Extension",
    "version": "1.0",

    "description": "Completing the tutorial",
    "homepage_url": "https://github.com/amdeason/Test_Extension",
    "icons": {
        "48": "icon-48.png"
    }, 
    "permissions": ["activeTab"],

    "browser_action": {
        "default_icon": "icon-48.png",
        "default_title": "Test_Extension",
        "default_popup": "popup.html"
    },

    "web_accessible_resources": [
        "beasts/frog.jpg",
        "beasts/turtle.jpg",
        "beasts/snake.jpg"
    ]
}